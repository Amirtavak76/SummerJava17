package entityTests;

import entities.Course;
import entities.Person;

public class CourseTest
{
	//Constructor Test;
	
	public boolean constructorTest()
	{
		Course Test = new Course("Intermidiate");
		String temp = new String("Intermidiate" + null  + null + 0 + null);
		if (temp.equals(Test.toString()))
		{
			return true;
		}
		else
		{
			return false;
		}
	}
	
	//Getter Test:
	
		public boolean getterTest()
		{
			Course getterTest = new Course("Intermidiate");
			String expected = "Intermidiate";
			String actual = getterTest.getName();
			if (actual.equals(expected))
			{
				return true;
			}
			else
			{
				return false;
			}
		}
}
