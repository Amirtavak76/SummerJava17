package entities;

import entityTests.PersonTest;
import entityTests.SchoolTest;

public class Main
{
	public static void main(String args[])
	{
		
		//Test of Constructor of Person 
		
		
		PersonTest test1 = new PersonTest();
		if(test1.constructorTest())
		{
			System.out.println("The constructor of Person class is Okay");
		}
		else
		{
			System.out.println("Wrong");
		}
		
		//Test of Constructor of School
		
		SchoolTest test2 = new SchoolTest();
		if(test2.constructorTest())
		{
			System.out.println("The constructor of School class is Okay");
		}
		else
		{
			System.out.println("Wrong");
		}
	}
}
